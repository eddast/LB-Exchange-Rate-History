{
  "name": "axolot-client",
  "version": "1.0.0",
  "description": "Axolot Client Front End - FlashCards and Study Help",
  "main": "index.tsx",
  "repository": "",
  "author": "Arnar Freyr and Edda Steinunn",
  "private": true,
  "scripts": {
    "start": "node set_env.js prod&./node_modules/.bin/webpack-dev-server --history-api-fallback --progress",
    "start:dev": "node set_env.js dev& ./node_modules/.bin/webpack-dev-server --history-api-fallback --progress",
    "build": "node set_env.js prod&webpack",
    "test": "jest --coverage -u"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "<rootDir>/__tests__/testShim.js",
      "<rootDir>/__tests__/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules",
      "node_modules/@types"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/__tests__/**/*.test.ts?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!src/index.tsx",
      "!src/view/App.tsx",
      "!src/services/ServeCustomers.tsx",
      "!src/services/authentication.ts"
    ]
  },
  "dependencies": {
    "@types/auth0-js": "^8.11.2",
    "@types/base-64": "^0.1.2",
    "@types/utf8": "^2.1.6",
    "auth0-js": "^9.6.1",
    "base-64": "^0.1.0",
    "css-loader": "^0.28.10",
    "file-loader": "^1.1.11",
    "file-saver": "^1.3.8",
    "kennitala-utility": "^4.2.5",
    "material-ui": "next",
    "material-ui-icons": "^1.0.0-beta.36",
    "moment": "^2.22.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.2.4",
    "@types/react-test-renderer": "^16.0.1",
    "@types/redux": "^3.6.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fs": "^0.0.1-security",
    "identity-obj-proxy": "^3.0.0",
    "img-loader": "^3.0.0",
    "jest": "^22.4.0",
    "react-addons-test-utils": "^15.6.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.3.2",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^22.4.1",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  }
}
